---
layout: null
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>{{ site.name }} by Brandon Keepers</title>
  <link href="{{ site.url }}"/>
  <link type="application/atom+xml" rel="self" href="{{ site.url }}{{ page.url }}"/>
  <updated>{{ site.posts.first.date | date_to_xmlschema }}</updated>
  <id>{{ site.url }}</id>
  <author>
    <name>Brandon Keepers</name>
    <email>brandon@opensoul.org</email>
  </author>
  {% for post in site.posts limit:15 %}
  <entry>
    <title>{{ post.title | xml_escape }}</title>
    <link type="text/html" rel="alternate" href="{{ site.url }}{{ post.url }}"/>
    <id>{{ post.id }}</id>
    <published>{{ post.date | date_to_xmlschema }}</published>
    {% for category in post.categories %}<category term="{{ category }}"/>{% endfor %}
    <summary type="html">{{ post.excerpt | xml_escape }}</summary>
    <content type="html">{{ post.content | xml_escape }}</content>
    <author>
      <name>Brandon Keepers</name>
      <uri>{{ site.url }}</uri>
    </author>
  </entry>
  {% endfor %}
</feed>
